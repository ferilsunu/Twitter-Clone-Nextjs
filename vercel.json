{
    "version": 2,
    "builds": [
      {
        "src": "vercel-build.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/vercel-build.js" }
    ],
    "env": {
      "PRISMA_TELEMETRY_INFORMATION": "false"
    }
  }
  